#include <stdio.h>

int main() {
    int a, b;
    printf("Ingrese dos números enteros: ");
    scanf("%d %d", &a, &b);

    if (a > b)
        printf("%d es mayor que %d\n", a, b);
    else if (a < b)
        printf("%d es mayor que %d\n", b, a);
    else
        printf("Ambos números son iguales\n");

    return 0;
}